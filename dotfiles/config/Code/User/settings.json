{
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Default Light+",
  "window.titleBarStyle": "custom",
  "workbench.iconTheme": "vscode-icons",
  "files.associations": {
    "*.yaml": "yaml"
  },
  "ansible.autocompletion": false,
  "ansible.hover": false,
  "ansible.validation": false,
  "json.format.enable": false,
  "yaml.completion": false,
  "git.autofetch": true,
  "go.toolsGopath": "/home/krieger/code/gopath",
  "go.formatTool": "gofmt",
  "go.useLanguageServer": true,
"editor.detectIndentation": false,
"editor.tabSize": 2,
"telemetry.enableTelemetry": false,
"[go]": {

  "editor.insertSpaces": false,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  }
},
"go.formatFlags": [
  "-s"
],
"workspaceExplorer.workspaceStorageDirectory": "/home/krieger/code/workspaces",
"workspaceExplorer.enableCustomIconSearch": false,
"workbench.editor.decorations.badges": true,
"workbench.editor.decorations.colors": true,
"gitlens.hovers.currentLine.over": "line",
"gitlens.codeLens.enabled": false,
"gitlens.currentLine.enabled": false,
"aws.telemetry": false,
"liveshare.authenticationProvider": "Microsoft"
}
